{
  "layouts" : {
    "basic": {
      "before": ["header","navbar","breadcrumbs"],
      "after": ["footer"]
    },
    "simple": {
      "before": ["header", "navbar"],
      "after": ["footer"]
    }
  },
  "globals" : {
        "app_name" : "LAYER",
        "app_version" : "0.0.2",
        "app_author"  : "Loris Rossi",
        "app_description" : "MVC PHP Framework",
        "app_keywords"  :  ["test","test2","test3"],
        "contact" : "webadmin@mail.com"
  },
  "locations" : {
      "models" : "app/models",
      "controllers": "app/services",
      "shared": "app/services/#shared",
      "libs": "app/vendor",
      "public": "public",
      "logs": "logs"
  },
  "environment" : {
      "current" : "dev",
      "dev": {
        "routeTemplate" : "",
        "apiRouteTemplate" : "api",
        "log": true,
        "logTemplate" : "[{request_datetime}][{environment}][{client_ip} {client_browser} {client_os}][{request_method} {request_resource}]:{message}",
        "build": true,
        "encryptedSession": true,
        "encryptedKey": "123456789"
      },
      "prod": {
        "routeTemplate" : "",
        "apiRouteTemplate" : "api",
        "log": true,
        "logTemplate" : "[{request_datetime}][{environment}][{client_ip} {client_browser} {client_os}][{request_method} {request_resource}]:{message}",
        "build": false,
        "encryptedSession": false,
        "encryptedKey": "123456789"
      }
  }
}